	<title>Assignment 1</title>
	<script = "text/javascript"> 
		var birthYear = parseInt(prompt("Enter your birthyear", ""));
		var currentYear = new Date().getFullYear();
		var newAge = currentYear - birthYear;
		document.write("You are " + newAge + "!");
	</script>
